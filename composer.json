{
    "name": "nickfairchild/kynda-installer",
    "description": "Kynda WordPress installer.",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Fairchild",
            "email": "nick.fairchild@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "symfony/console": "^4.0|^5.0",
        "symfony/process": "^4.2|^5.0"
    },
    "bin": [
        "bin/kynda"
    ],
    "autoload": {
        "psr-4": {
            "NickFairchild\\Installer\\Console\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
